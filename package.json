{
  "name": "flatten-this-json",
  "version": "1.0.0-beta.1",
  "description": "A lightweight JavaScript library to flatten nested JSON objects into a single-level structure.",
  "main": "index.js",
  "exports": {
    ".": {
      "browser": {
        "require": "./dist/browser/index.cjs",
        "default": "./index.js"
      },
      "default": {
        "require": "./dist/node/index.cjs",
        "default": "./index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "prebuild": "rimraf -rf dist",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:esm": "npm run test --prefix test/module/esm",
    "test:cjs": "npm run test --prefix test/module/cjs",
    "test:module": "npm run test:esm && npm run test:cjs",
    "test": "npm run test:module && npm run test:unit",
    "tdd": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "cleanup:esm": "rimraf test/module/cjs/node_modules",
    "cleanup:cjs": "rimraf test/module/esm/node_modules",
    "cleanup": "npm run cleanup:esm && npm run cleanup:cjs",
    "posttest:module": "npm run cleanup",
    "build": "rollup -c -m",
    "prepare": "husky",
    "cmt": "cz"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kishor82/flatten-this-json.git"
  },
  "keywords": [
    "flat-json",
    "json",
    "flatten",
    "flattening",
    "nested",
    "flat",
    "unflatten",
    "unflattening"
  ],
  "author": "Kishor Rathva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kishor82/flatten-this-json/issues"
  },
  "homepage": "https://github.com/kishor82/flatten-this-json#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.15.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3"
  }
}
