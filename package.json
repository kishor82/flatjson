{
  "name": "flatten-this-json",
  "version": "1.0.0-beta.1",
  "description": "A lightweight JavaScript library to flatten nested JSON objects into a single-level structure.",
  "main": "lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "require": "./dist/browser/index.cjs",
        "default": "./index.js"
      },
      "default": {
        "require": "./dist/node/index.cjs",
        "default": "./index.js"
      }
    }
  },
  "scripts": {
    "prebuild": "rimraf -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "tdd": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "build": "rollup -c -m"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kishor82/flatten-this-json.git"
  },
  "keywords": [
    "flat-json",
    "json",
    "flatten",
    "flattening",
    "nested",
    "flat",
    "unflatten",
    "unflattening"
  ],
  "author": "Kishor Rathva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kishor82/flatten-this-json/issues"
  },
  "homepage": "https://github.com/kishor82/flatten-this-json#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.15.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3"
  }
}
